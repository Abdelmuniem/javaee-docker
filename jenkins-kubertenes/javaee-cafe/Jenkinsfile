pipeline {
	agent any

tools {
	maven 'maven_3_5_4'
}


	stages {
		stage ('Build') {

			steps {
				//withMaven(maven : 'maven_3_5_4'){
					sh 'mvn clean package -f /jenkins-kubernetes/javaee-cafe/pom.xml'
				//}
			}
		}

		stage ('Deploy') {

			steps {
				//withMaven(maven : 'maven_3_5_4') {
					sh 'mvn deploy  -f /jenkins-kubernetes/javaee-cafe/pom.xml'
				//}
			}
		}
	}
}